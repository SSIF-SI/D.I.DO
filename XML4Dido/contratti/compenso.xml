<?xml version="1.0" encoding="UTF-8"?>

<masterDocument name="compenso">
	<inputs>
		<input>Descrittivo GAE</input>
		<input>Sigla CDR</input>
		<input>Progetto</input>
		<input>Destinatario</input>
		<input>Tipo contratto</input>
		<input>Riferimento contratto</input>
	</inputs>
	<list>
		<document name="compenso" minOccur="1" maxOccur="1">
			<signatures>
				<signature role="DIR" />
			</signatures>
			<inputs>
				<input>numero compenso</input>
			</inputs>
		</document>
		<document name="mandato" minOccur="1" maxOccur="1">
			<signatures>
				<signature role="SAMM" />
			</signatures>
			<inputs>
				<input>numero mandato</input>
				<input type="date">data mandato</input>
			</inputs>
		</document>
	</list>
</masterDocument>