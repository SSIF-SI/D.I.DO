<?xml version="1.0" encoding="UTF-8"?>

<masterDocument name="acquisto" owner="SA">
	<types>
		<type>mepa</type>
		<type>fuori mepa</type>
	</types>
	<inputs>
		<input from="geco" key="id_acquisto">Id Acquisto</input>
		<input from="geco" key="richiedente">Richiedente</input>
		<input from="geco" key="fornitore">Fornitore</input>
		<input>Descrittivo GAE</input>
		<input from="geco" key="laboratorio">Laboratorio</input>
		<input from="geco" key="natura">Natura</input>
		<input from="geco" key="natura">Voce del piano dei conti</input>
		<input from="geco" key="cup">CUP</input>
		<input>titolare dei fondi</input>
		<input>responsabile di laboratorio</input>
	</inputs>
	<list>
		<document load="proposta ordine.xml" />
		<document name="indagine di mercato" minOccur="1" maxOccur="1"
			onlyIfType="mepa" />
		<document name="preventivo/richiesta preventivo" minOccur="2"
			onlyIfType="fuori mepa" />
		<document name="foglio cig" minOccur="0" maxOccur="1">
			<inputs>
				<input>numero cig</input>
			</inputs>
		</document>
		<document name="determina" minOccur="1" maxOccur="1" />
		<document name="provvedimento d'impegno" minOccur="1"
			maxOccur="1">
			<inputs>
				<input>numero impegno</input>
				<input type="year">anno impegno</input>
			</inputs>
			<signatures>
				<signature role="DIR" />
				<signature role="SAMM" />
			</signatures>
		</document>
		<document name="conflitto d'interesse" minOccur="1" maxOccur="1">
			<signatures>
				<signature role="RUP" />
			</signatures>
		</document>
		<document name="ordine mepa-sigla" minOccur="1" maxOccur="1">
			<signatures>
				<signature role="DIR" />
			</signatures>
		</document>
		<document name="documento di rettifica" minOccur="0" maxOccur="1"/>
		<document name="fattura" minOccur="1" maxOccur="1">
			<inputs>
				<input>numero fattura</input>
				<input type="data">data fattura</input>
			</inputs>
		</document>
		<document name="verbale di collaudo" minOccur="0" maxOccur="1" />
		<document name="dichiarazione dei requisiti" minOccur="1"
			maxOccur="1" />
		<document name="modello di liquidazione" minOccur="1"
			maxOccur="1">
			<signatures>
				<signature role="RUP" />
			</signatures>
		</document>
		<document name="mandato di pagamento" minOccur="1" maxOccur="1">
			<inputs>
				<input>numero mandato</input>
				<input type="data">data mandato</input>
			</inputs>
			<signatures>
				<signature role="SAMM" />
			</signatures>
		</document>
	</list>
</masterDocument>