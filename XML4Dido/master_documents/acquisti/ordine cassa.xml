<?xml version="1.0" encoding="UTF-8"?>

<masterDocument name="ordine cassa" owner="SA" from="geco">
	<inputs>
		<input key="id_ordine">id ordine</input>
		<input key="ditta" shortView="true">fornitore</input>
		<input key="richiedente_cf" transform="idPersonaFromCf" values="persone" shortView="true">richiedente</input>
		<input key="destinatario_cf" transform="idPersonaFromCf" values="persone" shortView="true">destinatario</input>
		<input key="descrizione" type="textarea" shortView="true">descrizione</input>
		<input key="gae_nome" shortView="true">linea attività</input>
		<input key="gae_anno">linea attività anno</input>
		<input key="natura_finanziamento">natura finanziamento</input>
		<input key="piano_conto">voce del piano dei conti</input>
		<input key="codice_cup mandatory="0"">cup</input>
		<input key="titolare_fondi_cf" transform="idPersonaFromCf" values="responsabili" signRole="TF">titolare dei fondi</input>
		<input key="responsabile_lab_cf" transform="idPersonaFromCf" values="responsabili" signRole="RL">responsabile di laboratorio</input>
	</inputs>
	<list>
		<document load="proposta ordine.xml" />
		<document name="scontrini" minOccur="1" />
		<document name="quietanza di pagamento" minOccur="1"
			maxOccur="1">
			<inputs>
				<input type="data">data quietanza</input>
			</inputs>
		</document>
		<document name="documento generico" minOccur="1" maxOccur="1">
			<inputs>
				<input>numero documento</input>
				<input type="data">data documento</input>
			</inputs>
			<signatures>
				<signature role="DIR" />
			</signatures>
		</document>
	</list>
</masterDocument>