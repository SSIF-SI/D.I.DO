			<signatures>
				<signature role="DIR" />
			</signatures>
			<inputs>
				<input>numero compenso</input>
			</inputs>
		</document>
		<document name="mandato" minOccur="1" maxOccur="1">
			<signatures>
				<signature role="SAMM" />
			</signatures>
			<inputs>
				<input>numero mandato</input>
				<input type="date">data mandato</input>
			</inputs>
		</document>
	</list>
</masterDocument>