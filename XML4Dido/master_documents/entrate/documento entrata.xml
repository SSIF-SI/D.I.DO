<?xml version="1.0" encoding="UTF-8"?>

<masterDocument name="documento entrata" owner="SA">
	<inputs>
		<input shortView="true">descrittivo gae</input>
		<input values="gruppi" shortView="true">laboratorio</input>
		<input values="progetti" shortView="true">progetto</input>
	</inputs>
	<types>
		<type>fattura di entrata</type>
		<type>nota di debito</type>
	</types>
	<list>
		<document name="lettera" minOccur="1" maxOccur="1">
			<signatures>
				<signature role="REQ" />
			</signatures>
		</document>
		<document name="fattura di entrata" minOccur="1" maxOccur="1"
			onlyIfType="fattura di entrata">
			<inputs>
				<input>numero fattura</input>
				<input type="data">data fattura</input>
			</inputs>
		</document>
		<document name="nota di debito" minOccur="1" maxOccur="1"
			onlyIfType="nota di debito">
			<inputs>
				<input>numero nota di debito</input>
				<input type="data">data nota di debito</input>
			</inputs>
		</document>
	</list>
</masterDocument>
