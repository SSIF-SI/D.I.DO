<?xml version="1.0" encoding="UTF-8"?>
<!-- Dati da associare al documento: CDR/GAE/Progetto/natura/voce del piano 
	dei conti/CUP/PDG si-no/ -->

<masterDocument name="incasso">
	<inputs>
		<input>Descrittivo GAE</input>
		<input>Sigla CDR</input>
		<input>Progetto</input>
		<input>Natura</input>
		<input>Voce del piano dei conti</input>
		<input>CUP</input>
	</inputs>
	<types>
		<type>con accertamento</type>
		<type>con procedimento unico di accertamento</type>
	</types>
	<list>
		<document name="accertamento" minOccur="1" maxOccur="1"
			onlyIfType="con accertamento">
			<signatures>
				<signature role="DIR" />
			</signatures>
		</document>
		<document name="provvedimento unico di accertamento"
			minOccur="1" maxOccur="1" onlyIfType="con procedimento unico di accertamento">
			<signatures>
				<signature role="DIR" />
			</signatures>
		</document>
		<document name="documento generico di entrata" minOccur="0"
			maxOccur="1">
			<inputs>
				<input>numero documento</input>
				<input type="data">data documento</input>
			</inputs>
		</document>
		<attachments minOccur="0" />
		<document name="reversale di incasso" minOccur="1" maxOccur="1">
			<inputs>
				<input>numero reversale</input>
				<input type="data">data reversale</input>
			</inputs>
			<signatures>
				<signature role="SAMM" />
			</signatures>
		</document>
	</list>
</masterDocument>