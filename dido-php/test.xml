<?xml version="1.0" encoding="UTF-8"?>

<masterDocument name="missione">
	<types>
		<type>con anticipo</type>
		<type>senza anticipo</type>
	</types>
	<inputs>
		<input from="geco" key="id_missione">Id Missione</input>
		<input from="geco" key="nome|cognome" join=" ">Dipendente</input>
		<input from="geco" key="cf" type="hidden">cf</input>
		<input from="geco" key="data_inizio">Data inizio missione</input>
		<input from="geco" key="localita">Localit√†</input>
		<input>Descrittivo GAE</input>
		<input from="geco" key="cdr">Sigla CDR</input>
		<input from="geco" key="tipo_trattamento">Tipo trattamento</input>
		<input from="geco" key="cup">CUP</input>
		<input type="hidden">tipo</input>
	</inputs>
	<list>
		<document load="proposta d'ordine.xml" />
		<document name="richiesta di anticipo" minOccur="0" maxOccur="1"
			onlyIfType="con anticipo">
			<attachments minOccur="0" />
			<signatures>
				<signature role="REQ" />
				<signature role="DIR" />
			</signatures>
		</document>
		<document name="richiesta noleggio mezzo" minOccur="0"
			maxOccur="1">
			<attachments minOccur="0" />
			<signatures>
				<signature role="REQ" />
				<signature role="DIR" />
			</signatures>
		</document>
		<document name="autodichiarazione" minOccur="0"
			mandatoryIf="trattamento alternativo">
			<signatures>
				<signature role="REQ" />
			</signatures>
		</document>
		<document name="nota di liquidazione anticipo" minOccur="1"
			maxOccur="1" onlyIfType="con anticipo">
			<inputs>
				<input>numero nota</input>
				<input type="data">data nota</input>
			</inputs>
			<signatures>
				<signature role="DIR" />
			</signatures>
		</document>
		<document name="mandato di anticipo" minOccur="1" maxOccur="1"
			onlyIfType="con anticipo">
			<inputs>
				<input>numero mandato</input>
				<input type="data">data mandato</input>
			</inputs>
			<signatures>
				<signature role="SAMM" />
			</signatures>
		</document>
		<document name="richiesta rimborso" minOccur="1" maxOccur="1">
			<attachments minOccur="1" />
			<signatures>
				<signature role="REQ" />
				<signature role="DIR" afterValidation="DIR,AMM" />
			</signatures>
		</document>
		<document name="nota di liquidazione" minOccur="1" maxOccur="1">
			<inputs>
				<input>numero nota</input>
				<input type="data">data nota</input>
			</inputs>
			<signatures>
				<signature role="DIR" />
			</signatures>
		</document>
		<document name="mandato di missione" minOccur="1" maxOccur="1">
			<inputs>
				<input>numero mandato</input>
				<input type="data">data mandato</input>
			</inputs>
			<signatures>
				<signature role="SAMM" />
			</signatures>
		</document>
	</list>
</masterDocument>