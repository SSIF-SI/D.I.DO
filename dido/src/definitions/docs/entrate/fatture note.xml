<?xml version="1.0" encoding="UTF-8"?>

<masterDocument name="fatture note">
	<types>
		<type>fattura di entrata</type>
		<type>nota di debito</type>
		<type>nota di credito</type>
	</types>
	<list>
		<document name="lettera" minOccur="1" maxOccur="1">
			<signatures>
				<signature role="REQ"/>
			</signatures>
		</document>
		<document name="fattura di entrata" minOccur="1" maxOccur="1" onlyIfType="fattura di entrata" />
		<document name="nota di debito" minOccur="1" maxOccur="1" onlyIfType="nota di debito" />
		<document name="nota di credito" minOccur="1" maxOccur="1" onlyIfType="nota di credito" />
		<attachments minOccur="0" />
	</list>	
</masterDocument>