<?xml version="1.0" encoding="UTF-8"?>

<masterDocument name="incasso">
	<types>
		<type>con accertamento</type>
		<type>con procedimento unico di accertamento</type>
	</types>
	<list>
		<document name="accertamento" minOccur="1" maxOccur="1" onlyIfType="con accertamento">
			<signatures>
				<signature role="DIR"/>
			</signatures>
		</document>
		<document name="provvedimento unico di accertamento" minOccur="1" maxOccur="1" onlyIfType="con procedimento unico di accertamento">
			<signatures>
				<signature role="DIR"/>
			</signatures>
		</document>
		<document name="documento generico di entrata" minOccur="0" maxOccur="1" />
		<attachments minOccur="0" />
		<document name="reversale di incasso" minOccur="1" maxOccur="1">
			<signatures>
				<signature role="SAMM"/>
			</signatures>
		</document>
	</list>	
</masterDocument>